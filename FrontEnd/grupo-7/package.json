{
  "name": "grupo-7",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.24.0",
    "coverage": "^0.4.1",
    "formik": "^2.2.9",
    "google-map-react": "^2.1.10",
    "i18next": "^21.5.4",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "js-cookie": "^3.0.1",
    "leaflet": "^1.7.1",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-calendar": "^3.5.0",
    "react-country-flag": "^3.0.2",
    "react-custom-scrollbars-2": "^4.4.0",
    "react-datepicker": "^4.3.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.3",
    "react-image-gallery": "^1.2.7",
    "react-leaflet": "^3.2.2",
    "react-responsive-carousel": "^3.2.22",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "sweetalert2": "^11.1.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/assetsTransformer.js"
    },
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "lines": 40,
        "statements": 40
      }
    },
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{jsx,js}",
      "!src/i18n.js",
      "!src/index.js",
      "!src/Components/Card/Card.jsx",
      "!src/Components/Context/UserContext.jsx",
      "!src/Components/Context/UsuarioContext.jsx",
      "!src/Components/Administration/ProductEditionFormik.jsx",
      "!src/Components/Administration/ProductRegistrationFormik.jsx",
      "!src/Components/RegisterFormik/RegisterFormik.jsx",
      "!src/Components/BookingFormik/BookingFormik.jsx",
      "!src/Components/LoginFormik/LoginFormik.jsx",
      "!src/Components/Header/Header.jsx",
      "!src/Components/LanguageMenu/LanguageMenuDesktop.jsx",
      "!src/Components/LanguageMenu/LanguageMenuMobile.jsx",
      "!src/Components/LanguageMenu/LanguageMenuTablet.jsx"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-plugin-import": "^2.25.3"
  },
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "unmockedModulePathPatterns": [
    "node_modules/react/",
    "node_modules/enzyme/"
  ],
  "setupFilesAfterEnv": [
    "jest-enzyme"
  ],
  "testEnvironment": "enzyme",
  "testEnvironmentOptions": {
    "enzymeAdapter": "react16"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
